nt\";\r\nimport 'moment/locale/es';\r\n\r\nclass Articles extends Component {\r\n\r\n    url = Global.url;\r\n\r\n    state = {\r\n        articles: [],\r\n        status: null\r\n    };\r\n\r\n    /* El Component Will mount se usa para cargar antes de la vista */\r\n    componentDidMount(){\r\n        var home = this.props.home;\r\n        var search = this.props.search;\r\n        var blog = this.props.blog;\r\n\r\n        if(home === \"true\"){\r\n            this.getLastArticles();\r\n        \r\n        }else if(search && search !== null && search !== undefined){\r\n            this.getArticlesBySearch(search);\r\n\r\n        } else { \r\n                //this.getArticles();\r\n                this.getLastArticles();\r\n        }\r\n    }\r\n    \r\n    getArticlesBySearch = (searched) => {\r\n        axios.get(`${this.url}search/${searched}`)\r\n            .then( res => {      \r\n                    this.setState({\r\n                        articles: res.data.articles,\r\n                        status: 'success'\r\n                    });\r\n            })\r\n            .catch( err => {\r\n                this.setState({\r\n                    articles: {},\r\n                    status: 'success'\r\n                });\r\n            });\r\n    }\r\n\r\n    getLastArticles = () => {\r\n        axios.get(`${this.url}articles/last`)\r\n            .then( res => {\r\n                this.setState({\r\n                    articles: res.data.articles,\r\n                    status: 'success'\r\n                })          \r\n            });\r\n    }\r\n\r\n    getArticles = () => {\r\n        axios.get(`${this.url}articles`)\r\n            .then( res => {\r\n                this.setState({\r\n                    articles: res.data.articles,\r\n                    status: 'success'\r\n                })\r\n            });\r\n    }\r\n    render() {\r\n        if(this.state.articles.length >= 1 && this.state.status === \"success\"){\r\n            var listArticles = this.state.articles.map((article) => {\r\n                return(\r\n                    <article key={article._id} className=\"article-item\" id=\"article-template\">\r\n                        <div className=\"image-wrap\">\r\n                            {/*article.image != null ? (\r\n                                <img src={this.url+'get-image/'+article.image} alt={article.title} />\r\n                            ):(\r\n                                <img src={ImageDefault} alt={article.title} />\r\n                            )\r\n\r\n                            {article.image != null ? (\r\n                                <img src={article.image} alt={article.title} />\r\n                            ):(\r\n                                <img src={ImageDefault} alt={article.title} />\r\n                            )\r\n\r\n                            */}\r\n                            {article.image != null ? (\r\n                                <img src={article.image} alt={article.title} />\r\n                            ):(\r\n                                <img src={ImageDefault} alt={article.title} />\r\n                            )\r\n                            }                            \r\n                        </div>\r\n    \r\n                        <h2>{article.title}</h2>\r\n                        <span className=\"date\">\r\n                            <Moment locale=\"es\" fromNow>{ article.date }</Moment>\r\n                        </span>\r\n                        <Link to={'/blog/articulo/'+article._id}>Leer más</Link>\r\n                        <div className=\"clearfix\"></div>\r\n                    </article>\r\n                );\r\n            });\r\n\r\n            return (\r\n                <div id=\"articles\">\r\n                    { listArticles }\r\n                </div>\r\n            );\r\n        }else if(this.state.articles.length === 0 && this.state.status === \"success\"){\r\n            return(\r\n                <div id=\"articles\">\r\n                    <h2 className=\"subheader\">No hay articulos para mostrar</h2>\r\n                    <p>Todavía no hay artículos en esta sección</p>\r\n                </div>\r\n\r\n            );\r\n        }else{\r\n            return (\r\n                <div id=\"articles\">\r\n                    <h2 className=\"subheader\">Cargando...</h2>\r\n                    <p>Espere mientras carga el contenido</p>\r\n                </div>\r\n\r\n            );\r\n        }\r\n    }\r\n}\r\n\r\nexport default Articles;","C:\\xampp\\htdocs\\practicasreact24012021\\practicasreact_roma\\aprendiendoreact\\src\\Global.js",[],"C:\\xampp\\htdocs\\practicasreact24012021\\practicasreact_roma\\aprendiendoreact\\src\\components\\Search.js",[],"C:\\xampp\\htdocs\\practicasreact24012021\\practicasreact_roma\\aprendiendoreact\\src\\components\\Article.js",[],"C:\\xampp\\htdocs\\practicasreact24012021\\practicasreact_roma\\aprendiendoreact\\src\\components\\CreateArticle.js",[],"C:\\xampp\\htdocs\\practicasreact24012021\\practicasreact_roma\\aprendiendoreact\\src\\components\\EditArticle.js",["251"],"import React, { Component } from 'react';\r\nimport { Redirect } from 'react-router-dom';\r\nimport axios from 'axios';\r\nimport Global from '../Global';\r\nimport Sidebar from './Sidebar';\r\nimport SimpleReactValidator from 'simple-react-validator';\r\nimport swal from 'sweetalert';\r\nimport ImageDefault from \"../assets/images/default.jpg\";\r\n\r\n//1. Tenemos que recoger el id del artículo a editar de la url\r\n//2. Crear un metodo para que saque ese objeto del backend\r\n//3. Repoblar o rellenar el formulario con esos datos\r\n//4. Actualizar el objeto haciendo una petición al backend\r\n\r\nclass EditArticle extends Component {\r\n\r\n    url = Global.url;\r\n\r\n    articleId = null;\r\n\r\n    titleRef = React.createRef();\r\n    contentRef = React.createRef();\r\n\r\n    state = {\r\n        article: {},\r\n        status: null,\r\n        selectedFile: null\r\n    };\r\n\r\n    articleId = this.props.match.params.id;\r\n\r\n    componentDidMount() {\r\n        this.getArticle(this.articleId);\r\n    }\r\n\r\n    validator = new SimpleReactValidator({\r\n        messages: {\r\n            required: 'Este campo es requerido'\r\n        }\r\n    });\r\n\r\n    /*getArticle = (Id) => {\r\n        axios.get(this.url + 'article/' + Id)\r\n            .then(res => {\r\n                this.setState({\r\n                    article: res.data.article\r\n                })\r\n            });\r\n    }*/\r\n\r\n    getArticle = (Id) => {\r\n \r\n\r\n        axios.get(this.url + 'article/' + Id)\r\n            .then(res => {\r\n                this.setState(\r\n                    {\r\n                        article: res.data.article\r\n                    })\r\n            }).catch(err => {\r\n                this.setState({\r\n                    articles: {}\r\n                });\r\n            });\r\n    }\r\n\r\n    /*handleChnage = (e) => {\r\n        this.setState({\r\n            article: {\r\n                title: e.target.value\r\n            }     \r\n        })\r\n    }*/\r\n\r\n\r\n    changeState = (e) => {\r\n        var article = {\r\n            title: this.titleRef.current.value,\r\n            content: this.contentRef.current.value,\r\n            image: this.state.article.image\r\n        };\r\n\r\n        this.setState({\r\n            article: article\r\n        });\r\n\r\n        this.validator.showMessages();\r\n        this.forceUpdate();\r\n    }\r\n\r\n    saveArticle = (e) => {\r\n        e.preventDefault();\r\n\r\n        if (this.validator.allValid()) {\r\n\r\n            axios.put(`${this.url}article/${this.articleId}`, this.state.article)\r\n                .then(res => {\r\n                    if (res.data.article) {\r\n                        this.setState({\r\n                            article: res.data.article,\r\n                            status: 'waiting'\r\n                        });\r\n\r\n                        swal(\r\n                            'Artículo editado',\r\n                            'El artículo ha sido editado correctamente',\r\n                            'success'\r\n                        );\r\n\r\n                        //Subir la imagen\r\n                        if (this.state.selectedFile !== null) {\r\n\r\n                            //Sacar el id del articulo  guardado\r\n                            var articleId = this.state.article._id;\r\n\r\n                            //Crear form data y añadir fichero\r\n                            const formData = new FormData();\r\n\r\n                            formData.append(\r\n                                'file0',\r\n                                this.state.selectedFile,\r\n                                this.state.selectedFile.name\r\n                            );\r\n\r\n                            //Petición ajax\r\n                            axios.post(this.url + 'upload-image/' + articleId, formData)\r\n                                .then(res => {\r\n                                    if (res.data.article) {\r\n                                        this.setState({\r\n                                            article: res.data.article,\r\n                                            status: 'success'\r\n                                        });\r\n                                    } else {\r\n                                        this.setState({\r\n                                            article: res.data.article,\r\n                                            status: 'failed'\r\n                                        });\r\n                                    }\r\n                                });\r\n\r\n                        } else {\r\n\r\n                            this.setState({\r\n                                status: 'success'\r\n                            });\r\n                        }\r\n                    } else {\r\n                        this.setState({\r\n                            status: 'failed'\r\n                        });\r\n                    }\r\n                });\r\n        } else {\r\n\r\n            this.setState({\r\n                status: 'failed'\r\n            });\r\n\r\n            this.validator.showMessages();\r\n            this.forceUpdate();\r\n        }\r\n    }\r\n\r\n    fileChange = (event) => {\r\n        this.setState({\r\n            selectedFile: event.target.files[0]\r\n        });\r\n    }\r\n\r\n    render() {\r\n\r\n        console.log(this.state.article);\r\n\r\n        if (this.state.article.title) {\r\n            var title = this.state.article.title;\r\n        }\r\n\r\n        if (this.state.status === 'success') {\r\n            return <Redirect to=\"/blog\" />;\r\n        }\r\n\r\n        var article = this.state.article;\r\n        return (\r\n            <div className=\"center\">\r\n                <section id=\"content\">\r\n                    <h1 className=\"subheader\">Editar articulo</h1>\r\n\r\n                    {/*Mostrar Datos del Formulario*/}\r\n                    {this.state.article.title &&\r\n\r\n                        <form className=\"mid-form\" onSubmit={this.saveArticle} onChange={this.changeState}>\r\n                            <div className=\"form-group\">\r\n                                <label htmlFor=\"title\">Título</label>\r\n                                <input type=\"text\" name=\"title\" defaultValue={article.title} ref={this.titleRef} />\r\n                                {this.validator.message('title', this.state.article.title, 'required|min:5|max:30|alpha_num_space')}\r\n                            </div>\r\n\r\n                            <div className=\"form-group\">\r\n                                <label htmlFor=\"content\">Contenido</label>\r\n                                <textarea type=\"text\" name=\"content\" defaultValue={article.content} ref={this.contentRef}></textarea>\r\n                                {this.validator.message('content', this.state.article.content, 'required|min:5|max:400|alpha_num_space')}\r\n                            </div>\r\n\r\n                            <div className=\"form-group\">\r\n                                <label htmlFor=\"file0\">Imagen</label>\r\n                                <input type=\"file\" name=\"file0\" onChange={this.fileChange} />\r\n                                <div className=\"image-wrap\">\r\n                                    {\r\n                                        article.image != null ? (\r\n                                            <img src={this.url + 'get-image/' + article.image} alt={article.title} className=\"thumb\" />\r\n                                        ) : (\r\n                                            <img src={ImageDefault} alt={article.title} className=\"thumb\" />\r\n                                        )\r\n\r\n                                    }\r\n                                </div>\r\n                            </div>\r\n                            <div class=\"clearfix\"></div>\r\n                            <input type=\"submit\" value=\"Guardar\" className=\"btn btn-success\" />\r\n                        </form>\r\n                    }\r\n                    {!this.state.article.title &&\r\n                        <h1 className=\"subheader\">Cargando...</h1>\r\n                    }\r\n\r\n\r\n\r\n                </section>\r\n                <Sidebar />\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default EditArticle;","C:\\xampp\\htdocs\\interconnetingSystems\\aprendiendoreact\\src\\index.js",[],["252","253"],"C:\\xampp\\htdocs\\interconnetingSystems\\aprendiendoreact\\src\\App.js",[],"C:\\xampp\\htdocs\\interconnetingSystems\\aprendiendoreact\\src\\reportWebVitals.js",[],"C:\\xampp\\htdocs\\interconnetingSystems\\aprendiendoreact\\src\\Router.js",[],"C:\\xampp\\htdocs\\interconnetin